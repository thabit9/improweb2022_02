@{
    Layout = null;
}

<ul class="list-group border_" style="border-radius: 0; max-height: 640px; overflow:scroll; overflow-x: hidden" id="style-10">
    @foreach (var category in Model)
    {
    <li class="list-group-item" style="padding-top:2px;padding-bottom:2px;">
        <a class="active" asp-controller="product" asp-action="category" asp-route-id="@category.Id"><i class="icon-chevron-right"></i>@category.Name </a> 
    </li>
    }
</ul>